    "@libs/AppRoutes": "file:./libs/AppRoutes",
    "@libs/AuthContext": "file:./libs/AuthContext",
    "@libs/Authorization": "file:./libs/Authorization",
    "@libs/test": "file:./libs/test",
    "@libs/utils": "file:./libs/utils",
    "client": "^0.0.1",
    "editorjs-text-alignment-blocktune": "^1.0.3",
    "express": "^5.2.1",
    "jodit-react": "^5.2.38",
    "js-cookie": "^3.0.5",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "libreoffice-convert": "^1.7.0",
    "mammoth": "^1.11.0",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "pdf-lib": "^1.17.1",
