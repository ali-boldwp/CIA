{
  "name": "devregion",
  "private": true,
  "scripts": {
    "setup": " npm i && npm install --prefix app/client && npm install --prefix app/server && npm run dev",
    "install:all": "npm i && npm install --prefix app/client && npm install --prefix app/server",
    "build": "npm i && npm run build --prefix app/client",
    "_dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "dev": "concurrently \"npm run dev --prefix app/server\"  \"npm run dev --prefix app/client\"",
    "live": "concurrently \"npm run dev --prefix app/server\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@editorjs/image": "^2.10.3",
    "@editorjs/underline": "^1.2.1",
